    "accordion-section-3": {
      "type": "section-accordions",
      "blocks": {
        "accordion_block_3_0": {
          "type": "text",
          "settings": {
            "title": "Do you offer warranty for your products?",
            "text": "<p>Items are covered by any <strong>brand\/manufacturer warranty <\/strong>that comes with the product. AUMI4 itself does not add an extra store warranty beyond your <strong>statutory consumer rights<\/strong>. If something arrives <strong>damaged or incorrect<\/strong>, start a return and we’ll make it right. Refund Policy is <a href=\"https:\/\/aumifour.com\/pages\/refund-return-policy\" title=\"https:\/\/aumifour.com\/pages\/refund-return-policy\">here<\/a> <\/p>"
          }
        },
        "accordion_block_3_1": {
          "type": "text",
          "settings": {
            "title": "Do you offer extended warranty?",
            "text": "<p>We<strong>do not <\/strong>sell extended warranties at this time. For defects or issues on arrival, use the returns process so we can assist.<\/p>"
          }
        },
        "accordion_block_3_2": {
          "type": "text",
          "settings": {
            "title": "How much does extended warranty cost?",
            "text": "<p> N\/A — we don’t offer extended warranties.<\/p>"
          }
        }
      },
