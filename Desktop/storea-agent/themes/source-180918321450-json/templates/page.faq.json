    "accordion-section-1": {
      "type": "section-accordions",
      "blocks": {
        "accordion_block_1_1": {
          "type": "text",
          "settings": {
            "title": "Is my payment data safe with you?",
            "text": "<p> Yes. Payments are processed through <strong>Shopify Payments, PayPal, and SolanaPay <\/strong>with secure encryption. We don’t store full card details, and your data is handled per our <a href=\"https:\/\/aumifour.com\/pages\/privacy-policy\" title=\"https:\/\/aumifour.com\/pages\/privacy-policy\">Privacy Policy.<\/a><\/p>"
          }
        },
        "accordion_block_1_2": {
          "type": "text",
          "settings": {
            "title": "Do you accept online payments?",
            "text": "<p>Absolutely. We accept <strong>credit\/debit cards, Shop Pay, PayPal, Apple Pay, Google Pay, and SolanaPay (crypto)<\/strong>. Availability can vary by country.<\/p>"
          }
        },
        "accordion_block_1_3": {
          "type": "text",
          "settings": {
            "title": "Can I pay in installments?",
            "text": "<p>Where available, <strong>Shop Pay or afterPay Installments <\/strong>can offer 4 interest‑free payments at checkout (subject to eligibility shown at checkout).<\/p>"
          }
        },
        "accordion_block_1_4": {
          "type": "text",
          "settings": {
            "title": "Where can I learn more about billing and taxes?",
            "text": "<p><a href=\"https:\/\/aumifour.com\/pages\/billing-policy\" title=\"https:\/\/aumifour.com\/pages\/billing-policy\">See our <strong>Billing Policy<\/strong><\/a><strong> <\/strong>for accepted methods, tax calculation at checkout, and how we handle discrepancies.<\/p>"
          }
        }
      },
